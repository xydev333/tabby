div.modal-body
    label Press the key now
    .input
        .stroke(*ngFor='let stroke of value')
            .key-container(*ngFor='let key of splitKeys(stroke); let isLast = last')
                .key {{key}}
                .plus(*ngIf='!isLast') +

    .timeout
        div([style.width]='timeoutProgress + "%"')
    a.btn.btn-default((click)='close()') Cancel
